    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.1",
    "react-dom": "^17.0.1",
    "resolve": "1.19.0",
    "semver": "7.3.4",
    "styled-components": "^5.2.1",
    "ts-pnp": "1.2.0",
    "typescript": "^4.1.3",
    "workbox-webpack-plugin": "6.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@svgr/webpack": "5.5.0",
    "@types/codemirror": "^0.0.102",
    "@types/dompurify": "^2.1.0",
    "@types/electron-load-devtool": "^1.2.0",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.166",
    "@types/nedb": "^1.8.11",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
