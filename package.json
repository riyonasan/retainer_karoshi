    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "5.0.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "electron": "^11.1.1",
