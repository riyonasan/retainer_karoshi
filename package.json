{
  "name": "retainer_karoshi",
  "version": "0.0.1",
  "description": "",
  "main": "public/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "yarn build:main && electron .",
    "start:inspect": "yarn build:main && electron --inspect=5858 .",
    "start:inspect-brk": "yarn build:main && electron --inspect-brk=5858 .",
    "develop": "webpack serve --host 0.0.0.0 --hot --config config/webpack.config.js --config-name renderer",
    "build:main": "webpack --config config/webpack.config.js --config-name main",
    "build:renderer": "webpack --config config/webpack.config.js --config-name renderer",
    "build": "webpack --config config/webpack.config.js --config-name main && webpack --config config/webpack.config.js --config-name renderer && electron-builder --win",
    "release": "webpack --config config/webpack.config.js --config-name main && webpack --config config/webpack.config.js --config-name renderer && electron-builder --win --publish always"
  },
  "build": {
    "appId": "com.riyonasan.retainer_karoshi",
    "productName": "Retainer Karoshi",
    "artifactName": "retainer-karoshi-${version}.${ext}",
    "win": {
      "target": "portable",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "publish": {
      "provider": "github",
      "owner": "riyonasan",
      "repo": "retainer_karoshi",
      "releaseType": "release"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "identity-obj-proxy": "3.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "fs-extra": "9.0.1",
    "lodash": "^4.17.20",
